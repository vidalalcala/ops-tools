<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>KeepAlive</key>
  <true/>
  <key>EnvironmentVariables</key>
    <dict>
           <key>MB_JETTY_PORT</key>
           <string>{{ metabase_port }}</string>
    </dict>
  <key>Label</key>
  <string>homebrew.mxcl.metabase</string>
  <key>ProgramArguments</key>
  <array>
    <string>{{ brew_prefix }}/bin/metabase</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>WorkingDirectory</key>
  <string>{{ brew_prefix }}/var/metabase</string>
  <key>StandardOutPath</key>
  <string>{{ brew_prefix }}/var/metabase/server.log</string>
  <key>StandardErrorPath</key>
  <string>{{ brew_prefix }}/var/metabase/error.log</string>
</dict>
</plist>
